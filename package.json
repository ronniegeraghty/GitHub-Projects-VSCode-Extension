{
  "name": "github-projects",
  "displayName": "GitHub Projects",
  "description": "Give you a view of the GitHub project you are working on. ",
  "repository": "https://github.com/ronniegeraghty/GitHub-Projects-VSCode-Extension.git",
  "author": "Ronnie Geraghty",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:gh-projects-view",
    "onCommand:gh-projects.refreshList",
    "onCommand:gh-projects.projectEdit",
    "onCommand:extension.test"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "github-projects",
          "title": "GitHub Projects",
          "icon": "resources/github.svg"
        }
      ]
    },
    "views": {
      "github-projects": [
        {
          "id": "gh-projects-view",
          "name": "GitHub Projects"
        },
        {
          "id": "github-settings-view",
          "name": "GitHub Settings"
        }
      ]
    },
    "commands": [
      {
        "command": "gh-projects.refreshList",
        "title": "Project Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "gh-projects.projectEdit",
        "title": "Project Edit",
        "icon": {
          "light": "resources/light/edit.svg",
          "dark": "resources/dark/edit.svg"
        }
      },
      {
        "command": "extension.test",
        "title": "Test Command"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "gh-projects.refreshList",
          "when": "view == gh-projects-view",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "gh-projects.projectEdit",
          "when": "view == gh-projects-view && viewItem == prject",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "typescript": "^3.7.5",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "github-api": "^3.3.0",
    "http": "0.0.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "simple-git": "^3.15.0",
    "vscode": "^1.1.36"
  }
}
